<Window x:Class="Patterns.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Patterns"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Button Content="Одиночка" x:Name="Btn" Click="Btn_Click" ></Button>
        <Button Content="Одиночка" Grid.Row="1" Click="Button_Click"></Button>
        <Button Content="Прототип" Click="Button_Click_1" Grid.Row="2"></Button>
        <Button Content="Одиночка + Прототип" Click="Button_Click_2" Grid.Row="3"></Button>
        <Button Content="Одиночка + Прототип" Click="Button_Click_3" Grid.Row="4"></Button>
        
    </Grid>
</Window>
